LogLevel ERROR

Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519
  IdentityFile ~/.ssh/id_rsa
{{- if .personal }}
  User stianrs
{{- end }}

{{- if (eq .chezmoi.os "darwin") }}
  IgnoreUnknown UseKeychain
  UseKeychain yes
{{- end }}

{{ if .personal -}}
{{ output "op" "read" "op://dotfiles/ssh config/notesPlain" }}
{{ end -}}